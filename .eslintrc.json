{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:markdown/recommended",
    "plugin:prettier/recommended",
  ],
  "overrides": [
    {
      "files": ["**/*.md"],
      "processor": "markdown/markdown"
  },
  ]
}